<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- View Tree -->
        <record id="view_management_tree" model="ir.ui.view">
            <field name="name">vehicle.management.tree</field>
            <field name="model">vehicle.management</field>
            <field name="arch" type="xml">
                <tree string="Vehicle Management Tree">
                    <field name="name"/>
                    <field name="model_id"/>
                    <field name="driver_id"/>
                    <field name="objective"/>
                    <field name="location"/>
                    <field name="distance"/>
                    <field name="start_date"/>
                    <field name="end_date"/>
                    <field name="state"/>
                    <field name="user_id"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>